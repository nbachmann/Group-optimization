   ],
   "source": [
    "#create model\n",
    "m = Model()\n",
    "\n",
    "\n",
    "#define variables#\n",
    "\n",
    "\n",
    "#student i is in group j\n",
    "x = m.addVars(26,8,vtype = GRB.BINARY)\n",
    "\n",
    "#corresponds to project status. y[j] = 0 if project is not happening, y[j] = 1 if it is.\n",
    "y = m.addVars(8,vtype = GRB.BINARY)\n",
    "\n",
    "#deciders for 3 or 4 people in group\n",
    "z1 = m.addVars(8,vtype = GRB.BINARY)\n",
    "z2 = m.addVars(8,vtype = GRB.BINARY)\n",
    "\n",
    "#figures out if group members can meet given their schedules. w[j,h] = 0 if all can meet.\n",
    "#w[j,h] = # of people who can't meet at [j,h] otherwise.\n",
    "w = m.addVars(8,98,vtype = GRB.INTEGER)\n",
    "\n",
    "#converts w[j,h] to binary. w_var[j,h] = 1 if all can meet at [j,h], w_var[j,h] = 0 otherwise.\n",
    "w_var = m.addVars(8,98,vtype = GRB.BINARY)\n",
    "\n",
    "#relaxation variables\n",
    "uc = m.addVar(vtype = GRB.INTEGER)\n",
    "uw = m.addVar(vtype = GRB.INTEGER)\n",
    "ul = m.addVar(vtype = GRB.INTEGER)\n",
    "up = m.addVar(vtype = GRB.INTEGER)\n",
    "ule = m.addVar(vtype = GRB.INTEGER)\n",
    "ut = m.addVar(vtype = GRB.INTEGER)\n",
    "uo = m.addVar(vtype = GRB.INTEGER)\n",
    "\n",
    "#update model\n",
    "m.update()\n",
    "\n",
    "\n",
    "#define constraints#\n",
    "\n",
    "\n",
    "#any student cannot be in more than 1 group\n",
    "m.addConstrs(((x.sum(i,'*')) == 1 for i in range(0,26)), name='students')\n",
    "\n",
    "#there are only 7 projects\n",
    "m.addConstr(y.sum() == 7)\n",
    "\n",
    "#there are no students in groups for projects that are not happening \n",
    "for j in range(0,8):\n",
    "    for i in range(0,26):\n",
    "        m.addGenConstrIndicator(y[j],0,x[i,j],GRB.EQUAL,0)        \n",
    "\n",
    "#there are either 3 or 4 students in a group\n",
    "m.addConstrs(((x.sum('*',j) == 3*z1[j] + 4*z2[j] for j in range(0,8))),name='threefour')\n",
    "m.addConstrs(((z1[j] + z2[j] <= 1 for j in range(0,8))),name='z constr')\n",
    "\n",
    "#at least 1 coder, writer, speaker, etc. in each group\n",
    "m.addConstrs(((quicksum(x[i,j]*code[i] for i in range(0,26))) >= 1 - uc for j in range(0,8)\n",
    "              if y[j] == 1), name='coders')\n",
    "m.addConstrs(((quicksum(x[i,j]*write[i] for i in range(0,26))) >= 1 - uw for j in range(0,8)\n",
    "              if y[j] == 1),name='writers')\n",
    "m.addConstrs(((quicksum(x[i,j]*latex[i] for i in range(0,26))) >= 1 - ul for j in range(0,8)\n",
    "              if y[j] == 1),name='latexers')\n",
    "m.addConstrs(((quicksum(x[i,j]*present[i] for i in range(0,26))) >= 1 - up for j in range(0,8)\n",
    "              if y[j] == 1),name='presenters')\n",
    "m.addConstrs(((quicksum(x[i,j]*lead[i] for i in range(0,26))) >= 1 - ule  for j in range(0,8)\n",
    "             if y[j] == 1),name='leaders')\n",
    "m.addConstrs(((quicksum(x[i,j]*manage[i] for i in range(0,26))) >= 1 - ut for j in range(0,8)\n",
    "             if y[j] == 1),name='time managers')\n",
    "m.addConstrs(((quicksum(x[i,j]*organize[i] for i in range(0,26))) >= 1 - uo for j in range(0,8)\n",
    "             if y[j] == 1),name='organizers')\n",
    "\n",
    "#define w\n",
    "m.addConstrs((w[j,h] == x.sum('*',j) - quicksum(a[i,h]*x[i,j] for i in range(0,26))\n",
    "              for j in range(0,8)\n",
    "             for h in range(0,98)))        \n",
    "\n",
    "#define w_var\n",
    "m.addConstrs((w[j,h] <= 4*(1 - w_var[j,h]) for j in range(0,8)\n",
    "              for h in range(0,98)),name='wvar1')\n",
    "m.addConstrs((w[j,h] >= 1 - 4*w_var[j,h] for j in range(0,8)\n",
    "              for h in range(0,98)),name='wvar2')\n",
    "m.addConstrs((w_var.sum(j,'*') >= 2 for h in range(0,98)\n",
    "             for j in range(0,8)),name='wvar3') \n",
    "\n",
    "#objective function - try to match project preference to ideal placement\n",
    "m.setObjective(quicksum(x[i,j]*pref[i,j] for i in range(0,26) for j in range(0,8)) - 15*uc - 5*uw - 5*ul - 5*up - 8*ule - 10*ut - 5*uo, GRB.MAXIMIZE)\n",
    "\n",
    "#optimize\n",
    "m.params.outputflag = 0\n",
    "m.optimize()"
   ]
  },
  {
   "cell_type": "code",
