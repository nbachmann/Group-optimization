   ],
   "source": [
    "#Format and save the project groups that were generated.\n",
    "\n",
    "x_arr = np.array([[0 for j in range(0,8)] for i in range(0,26)])\n",
    "times_arr = np.array([[0 for h in range(0,98)] for j in range(0,8)])\n",
    "proj_nums = np.array([0 for i in range(0,26)])\n",
    "optimalGroups = [[0 for j in range(0,26)] for i in range(0,2)]\n",
    "meeting_times = [[] for x in range(0,8)]\n",
    "\n",
    "for i in range(0,26):\n",
    "    for j in range(0,8):\n",
    "        x_arr[i,j] = int(x.select(i,'*')[j].X)\n",
    "\n",
    "for i in range(0,26):\n",
    "    for j in range(0,8):\n",
    "        if (x_arr[i,j] == 1):\n",
    "            proj_nums[i] = j\n",
    "\n",
    "for j in range(0,8):\n",
    "    for h in range(0,98):\n",
    "        times_arr[j,h] = int(w_var.select(j,'*')[h].X)\n",
    "\n",
    "#times in [h] that group [j] can meet at, in military time, for 7 days\n",
    "for j in range(0,8):\n",
    "    for h in range(0,98):\n",
    "        if times_arr[j,h] == 1:\n",
    "            if y[j] == 1: \n",
    "                meeting_times[j].append(h)"
   ]
  },
