   "source": [
    "#imports\n",
    "from gurobipy import*\n",
    "import pandas as pd\n",
    "import xlrd\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "#read and store excel file\n",
    "excel_file = pd.ExcelFile('/Users/nealbachmann/Groups.xlsx')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "#create dataframe map for all tabs of worksheet\n",
    "sheet_to_df_map = pd.read_excel(excel_file, sheet_name=None)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "#all skills and preferences\n",
    "\n",
    "#skills portion\n",
    "skills = sheet_to_df_map['Individual skills']\n",
    "\n",
    "# #save names\n",
    "# names = skills['Unnamed: 0']\n",
    "# names = names.values\n",
    "\n",
    "#drop unnecessary columns\n",
    "skills = skills.drop(columns=['Unnamed: 0','Unnamed: 8'])\n",
    "\n",
    "#project preference portion\n",
    "pref = sheet_to_df_map['Project Preferences']\n",
    "pref = pref.drop(columns=['Unnamed: 0', 'Unnamed: 9','Please rate each project on a scale of 0 to 10 then move on to the next sheet'])\n",
    "pref.columns = range(pref.shape[1])\n",
    "pref = pref.values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "#individual skills\n",
    "coded = skills['Coding skills']\n",
    "writed = skills['Writing skills']\n",
    "latexd = skills['Latex skills']\n",
    "presentd = skills['Presentation skills']\n",
    "leadd = skills['Leadership skills']\n",
    "organized = skills['Organizational skills']\n",
    "managed = skills['Time management skills']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "#availability\n",
    "a = sheet_to_df_map['Monday availabilities']\n",
    "\n",
    "#append all sheets of availabilites to one dataframe\n",
    "a = pd.concat([a, sheet_to_df_map['Tuesday availabilities']], axis=1)\n",
    "a.columns = list(range(len(a.columns)))\n",
    "a = pd.concat([a, sheet_to_df_map['Wednesday availabilities']], axis=1)\n",
    "a.columns = list(range(len(a.columns)))\n",
    "a = pd.concat([a, sheet_to_df_map['Thursday availabilities']], axis=1)\n",
    "a.columns = list(range(len(a.columns)))\n",
    "a = pd.concat([a, sheet_to_df_map['Friday availabilities']], axis=1)\n",
    "a.columns = list(range(len(a.columns)))\n",
    "a = pd.concat([a, sheet_to_df_map['Saturday availabilities']], axis=1)\n",
    "a.columns = list(range(len(a.columns)))\n",
    "a = pd.concat([a, sheet_to_df_map['Sunday availabilities']], axis=1)\n",
    "a.columns = list(range(len(a.columns)))\n",
    "\n",
    "#remove non-numbers\n",
    "a = a.select_dtypes(exclude=['object'])\n",
    "\n",
    "#remove null entries\n",
    "a = a.dropna(thresh=len(a) - 1, axis=1)\n",
    "\n",
    "#reset indices\n",
    "a.columns = range(a.shape[1])\n",
    "a = a.values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "#set proficiency bounds for skills and assign binary values, i.e. someone is considered a proficient\n",
    "#coder if they rated themself an 8 or above.\n",
    "\n",
    "code = (coded>=8)\n",
    "code*=1\n",
    "code = code.values\n",
    "\n",
    "write = (writed>=7)\n",
    "write*=1\n",
    "write = write.values\n",
    "\n",
    "latex = (latexd>=7)\n",
    "latex*=1\n",
    "latex = latex.values\n",
    "\n",
    "present = (presentd>=7)\n",
    "present*=1\n",
    "present = present.values\n",
    "\n",
    "lead = (leadd>=7)\n",
    "lead*=1\n",
    "lead = lead.values\n",
    "\n",
    "organize = (organized>=6)\n",
    "organize*=1\n",
    "organize = organize.values\n",
    "\n",
    "manage = (managed>=7)\n",
    "manage*=1\n",
    "manage = manage.values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
